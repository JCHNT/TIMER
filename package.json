{
  "name": "timer-ecos",
  "version": "2.0.0",
  "description": "Timer ECOS - Système de chronométrage pour examens universitaires",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "docker:build": "docker-compose build",
